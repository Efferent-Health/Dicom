{
    "compilerOptions": {
        "target": "ES2018",
        "module": "none",
        "outFile": "dist/Efferent.Dicom.js",
        "sourceMap": false,
        "declaration": true,
        "declarationMap": false,
        "removeComments": true,
        "strict": false,
        "lib": [
            "ES2020",
            "DOM"
        ],
        "skipLibCheck": true,
        "downlevelIteration": true,
        "noEmitOnError": false
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/index.node.ts"
    ]
}